{
  "name": "project-432",
  "version": "1.0.0",
  "description": "project-432",
  "main": "index.js",
  "scripts": {
    "build": "npm run build-clean && npm run build-babel && npm run build-copy",
    "build-clean": "rm -rf dist",
    "build-babel": "babel src --out-dir dist",
    "build-copy": "cp -R data dist/data",
    "dev": "nodemon --exec node -r node_modules/dotenv/config --watch src --exec babel-node ./src/index.js",
    "start": "nodemon --exec node -r node_modules/dotenv/config --watch src --exec babel-node ./src/index.js",
    "test": "jest",
    "serve": "node dist/index.js"
  },
  "author": "Rory Dent",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1"
  },
  "dependencies": {
    "express": "^4.17.1"
  }
}
